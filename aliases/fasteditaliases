# setup
        alias  ed='vim ~/.dotfiles/dotfiles-setup.sh'

# dotfiles
        alias  ez='vim ~/.dotfiles/zsh/.zshrc'
        alias  eb='vim ~/.dotfiles/bash/.bashrc'
        alias ebp='vim ~/.dotfiles/bash/.bash_profile'
        alias  eg='vim ~/.dotfiles/git/.gitconfig'
        alias  ev='vim ~/.dotfiles/vim/.vimrc'

# edit aliases
        alias efea='vim ~/.dotfiles/aliases/fasteditaliases'
        alias  ega='vim ~/.dotfiles/aliases/gitaliases'
        alias  esa='vim ~/.dotfiles/aliases/aliases'

# organisational
        alias et='vim ~/.priv/todo.wofl'
        alias st='cat ~/.priv/todo.wofl | grep "^\s*+"'
        alias ep='vim ~/.priv/priority.wofl'
        alias ea='vim ~/.priv/aim.wofl'
